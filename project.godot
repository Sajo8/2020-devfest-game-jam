config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "GeneralDino",
"language": "GDScript",
"path": "res://actors/dinos/GeneralDino.gd"
}, {
"base": "Button",
"class": "UpgradeButton",
"language": "GDScript",
"path": "res://GUI/buttons/UpgradeButton.gd"
} ]
_global_script_class_icons={
"GeneralDino": "res://assets/dinos/mega_dino/mega_dino.png",
"UpgradeButton": ""
}

[application]

config/name="Scaling Out"
config/description="Project for DevFest 2020 Game Jam.

Go on undercover missions, upgrade your dinos, and destroy some cities!"
run/main_scene="res://combat/CombatScreen.tscn"
config/icon="res://assets/dinos/mega_dino/mega_dino.png"
config/windows_native_icon="res://assets/dinos/mega_dino.ico"

[autoload]

SceneChanger="*res://singletons/SceneChanger.tscn"
DinoInfo="*res://singletons/DinoInfo.gd"
CombatInfo="*res://singletons/CombatInfo.gd"
Signals="*res://singletons/Signals.gd"
ShopInfo="*res://singletons/ShopInfo.gd"

[debug]

gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"

